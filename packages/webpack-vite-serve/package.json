{
  "name": "webpack-vite-serve",
  "version": "0.1.2",
  "description": "为webpack项目提供一键接入Vite的能力",
  "main": "dist/index.js",
  "bin": {
    "wvs": "dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "rimraf dist && tsup --minify",
    "prepublish": "npm run build",
    "up:major": "npm version major",
    "up:minor": "npm version minor",
    "up:patch": "npm version patch",
    "up:beta": "npm version prerelease --preid=beta",
    "publish:beta": "npm publish --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATQQ/webpack-vite-serve.git"
  },
  "keywords": [
    "vite",
    "webpack",
    "vue",
    "react",
    "vite-plugin"
  ],
  "author": "sugar",
  "license": "MIT"
  ,
  "bugs": {
    "url": "https://github.com/ATQQ/webpack-vite-serve/issues"
  },
  "homepage": "https://github.com/ATQQ/webpack-vite-serve#readme",
  "dependencies": {
    "@vitejs/plugin-react": "^1.1.0",
    "@vitejs/plugin-vue": "^1.10.1",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "cross-spawn": "^7.0.3",
    "ejs": "^3.1.6",
    "inquirer": "^8.2.0",
    "vite": "^2.6.14",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-vue2": "^1.9.0",
    "wp2vite": "^2.0.8"
  },
  "devDependencies": {}
}
